/*==============================*//* HORIZONTAL  *//*==============================*/#@{twoJObjectId}.twoj_slidertabs_horizontal {	> div.st_views{ margin-bottom: @tabHeight+@tabOutPx; }		> div.st_tabs{ margin-bottom: @tabOutPx;  		ul li a{ 			border-bottom: @borderStyle;			&.st_tab_active{ padding-bottom: 0; }		}	}	/* Bottom  Align*/	&.align_bottom{		 > div.st_tabs { 			bottom: -(@tabHeight+1px+@tabOutPx);			ul li a.st_tab_active { border-top: @borderStyle; }		}	}}/*==============================*//* VERTICAL  *//*==============================*/#@{twoJObjectId}.twoj_slidertabs_vertical {     > div.st_tabs {		margin-right: -@tabBorderWidth+@tabOutPx;        ul li a.st_tab_active {  padding-right:0; }    }		&.align_right > div.st_tabs {		margin-left: -@tabBorderWidth + @tabOutPx;		margin-right:0px;		ul li a.st_tab_active{			padding-right:0px;			border-left: @borderStyle;		}	}}