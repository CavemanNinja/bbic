    <copyright>Copyright (C) 2014 Fastw3b. All rights reserved.</copyright>
    <license>http://www.gnu.org/copyleft/gpl.html GNU/GPL</license>
	<version>1.0.0</version>
	<description><![CDATA[
		FW Gallery map module
	]]></description>
	<files>
		<filename module="mod_fwgallery_map">mod_fwgallery_map.php</filename>
		<filename>index.html</filename>
		<folder>assets</folder>
		<folder>tmpl</folder>
	</files>
	<languages folder="language">
		<language tag="en-GB">en-GB/en-GB.mod_fwgallery_map.ini</language>
	</languages>
	<config>
		<fields name="params">
			<fieldset name="basic">
				<field name="moduleclass_sfx" default="" type="text" label="COM_MODULES_FIELD_MODULECLASS_SFX_LABEL" description="COM_MODULES_FIELD_MODULECLASS_SFX_DESC" />
				<field name="height" type="text" default="300" label="Map height" description="" />
			</fieldset>
		</fields>
	</config>
