#@{twoJObjectId}{	 > div.st_views {		position:relative;		z-index: @zIndex + 5;		height:400px;		line-height:19px;		overflow:hidden;		border:@borderStyle;		.setBackgroundColor;		div.st_view {            width:100%;            height:100%;            overflow:hidden;            background-image:none;            background-repeat:no-repeat !important;            background-position:0px 0px !important;            background-color:transparent !important;			-webkit-font-smoothing:antialiased;        }		div.st_view_inner { padding: @contentPadding; }		div.st_view_first { display:block; }	 }	 > div.st_tabs{		ul{  padding:0px; list-style:none; margin:0px;			li{ float:left; margin:0px; background-image: none; padding: 0;				a{ margin:0px; display:block;  text-decoration:none;  outline:none; 					color: @tabColor; 					background: @tabBgColor;					i{						/* display:block;  */						height: @tabHeight;						line-height: @tabHeight;						vertical-align: middle;						text-align: center;						text-decoration: none;					}					&.st_tab_active{						color:@tabColorActive; 						background: @tabBgColorActive;					}					&:hover {						color:@tabColorHover; 						background:@tabBgColorHover; 					}					span {                        /* display:block;                        clear:both;                        font: @tabSpanFontWidth @tabSpanFontSize/@tabHeight @tabSpanFontName;                        color:#838383;                        padding-top:3px;						line-height:0px; */                    }				}			}		}		div.st_tabs_wrap { position:relative !important; }		a.st_next,		a.st_prev{			display:none;			top:0px;			z-index: @zIndex + 110;			outline:none;			color: @navButColor;			background-color: @navBgColor;			border:@borderStyle;			i{				margin: 0 auto;				display:block;				width:  @navButWidth; 				height: @tabHeight;				line-height: @tabHeight;				vertical-align: middle;				text-align: center;				text-decoration: none;			}			&.st_btn_disabled{ 				&:hover {  color: @navButColorDisabled; }				color: @navButColorDisabled;				background-color: @navBgColorDisabled;			}			&:hover { 				color: @navButColorHover;				text-decoration: none;				background-color: @navBgColorHover;			}		}	}}/*==============================*//* HORIZONTAL  *//*==============================*/#@{twoJObjectId}.twoj_slidertabs_horizontal {	> div.st_views{		padding:0px !important;		div.st_view { -moz-box-sizing:border-box; -webkit-box-sizing:border-box; box-sizing:border-box;  }	}	> div.st_tabs{		height: @tabHeight+2*@tabBorderWidth;		margin-bottom: -@tabBorderWidth;		div.st_tabs_wrap{ width:100%; }		ul{ width:10000px;			li{  display:block; 				a{ 					height: @tabHeight;					padding: @pendingButtonTop @pendingButtonRight @pendingButtonBottom @pendingButtonLeft;  /*padding: 0 @tabBorderWidth+12px; (@tabHeight-15px) */					font:  @tabFontWidth @tabFontSize/@tabHeight @tabFontName;	/* 43px */					border-top: @borderStyle;					border-right: @borderStyle;					&.st_tab_first{						border-left: @borderStyle;					}					&.st_tab_active{						padding-bottom:@tabBorderWidth + 1px;   /* effect offset */					}				}			}		}		a.st_prev,		a.st_next{			position:absolute;			height: @tabHeight;				width:  @navButWidth;		}	}	&.st_sliding_active{		> div.st_tabs {			height: @tabHeight+2*@tabBorderWidth;	 /* height:45px; */							div.st_tabs_wrap{ 							width:auto !important;				margin:0px @navButWidth+@tabBorderWidth;			}						ul li a{				&.st_tab_first{ 					border-left:0 none;				}				&.st_tab_last{					border-right:0 none;				}			}		}	}	/* Bottom  Align*/	&.align_bottom{		margin-bottom: @tabHeight+2*@tabBorderWidth/* 45px */;		 > div.st_tabs { 			position:absolute; 			bottom: -(@tabHeight+@tabBorderWidth) /* 44px */;			left:0px;			margin-bottom:0px;			ul li a{				border-bottom:@borderStyle;				&.st_tab_active {					padding-bottom:0px;					border-top-color: @tabBgColorActive; 				}			}		}		&.st_sliding_active{			> div.st_tabs{				border-top:0 none;			}		}	}}/*==============================*//* VERTICAL  *//*==============================*/#@{twoJObjectId}.twoj_slidertabs_vertical { /* div.twojSlidertabsClass1-vertical */     > div.st_views {        div.st_view_inner { overflow:hidden !important; }    }     > div.st_tabs {        position:relative;        z-index: @zIndex + 100;        float:left;        width: (@tabWidth + @pendingButtonLeft + @tabBorderWidth*2);/*  width: (@tabWidth + @tabLeftPadding + @tabBorderWidth*2); 189px; */        height: @verticalHeight;        margin-right: -@tabBorderWidth;        border-top: @borderStyle;        overflow:hidden;		div.st_tabs_wrap { 			float:left; 			clear:both; 		}		        ul{            float:left;            width: ( @tabWidth + @pendingButtonLeft + @tabBorderWidth);/* width: ( @tabWidth + @tabLeftPadding + @tabBorderWidth); 188px; */            li{                position:relative;                clear:both;                a{                    width: @tabWidth; /* 167px; */                    height: @tabHeight; /*32px; */                    padding:@pendingButtonTop 0 @pendingButtonBottom @pendingButtonLeft; /* 20px */                    font:@tabFontWidth @tabFontSize/@tabHeight @tabFontName; /* @tabfont */                    border-right:	@borderStyle;                    border-bottom:	@borderStyle;                    border-left:	@borderStyle;					&.st_tab_active {						position:relative;						z-index: @zIndex + 10;						padding-right: @tabBorderWidth;					}				}            }        }		a.st_prev,		a.st_next{			float:left;			position:relative;			height: @tabHeight;			/* width: (@tabWidth+@tabLeftPadding - @tabBorderWidth) / 2px ; */			width: (@tabWidth+@pendingButtonLeft - @tabBorderWidth) / 2px ;						border-top:0 none;		}        a.st_prev {            border-right:0 none;        }		    }	&.align_right > div.st_tabs {		float:right;		margin-left: -@tabBorderWidth;		margin-right:0px;		ul li a.st_tab_active{			padding-right:0px;			border-left-color: @tabBgColorActive;		}		a.st_prev { margin-left:0px; }		a.st_next { margin-right: -@tabBorderWidth; }	}}